<%= category[0].name %>
<a href="categories/<%= category.name %>/edit">Edit</a>
<a href="categories/<%= category.name %>/delete">Delete</a>

<% if (tours.length > 0) { %>
<h2>Tours in Category</h2>
<% tours.forEach(tour => { %>
    <h4><%= tour.title %> </h4> 
    <h3><%= tour.description %> </h3>
    <h3><%= tour.location %> </h3>
    <h3>Category: <%= tour.name %> </h3>
<% }); %> 
<% } %>
 

<a href="/categories">Back to Categories </a>